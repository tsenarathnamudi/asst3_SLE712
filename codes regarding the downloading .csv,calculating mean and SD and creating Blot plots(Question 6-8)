download.file("https://raw.githubusercontent.com/markziemann/SLE712_files/master/week10_files/growth_data.csv",destfile = "growth_data.csv")
h=read.csv("https://raw.githubusercontent.com/markziemann/SLE712_files/master/week10_files/growth_data.csv",header=T,sep=",")
h
summary(h)
mean_northeast1999cm_start<-mean(as.matrix(h[1:50,3]))
sd_northeast1999cm_start<-sd(as.matrix(h[1:50,3]))
mean_northeast1999cm_start
sd_northeast1999cm_start
mean_northeast2019cm_final<-mean(as.matrix(h[1:50,6]))
sd_northeast2019cm_final<-sd(as.matrix(h[1:50,6]))
mean_northeast2019cm_final
sd_northeast2019cm_final
mean_southwest1999cm_start<-mean(as.matrix(h[51:100,3]))
sd_southwest1999cm_start<-sd(as.matrix(h[51:100,3]))
mean_southwest1999cm_start
sd_southwest1999cm_start
mean_southwest2019cm_final<-mean(as.matrix(h[51:100,6]))    
sd_southwest2019cm_final<-sd(as.matrix(h[51:100,6]))
mean_southwest2019cm_final
sd_southwest2019cm_final
mean_southwest_initial
boxplot(h$Circumf_1999_cm[1:50],ylab="North east start-Circumf_1999_cm")
